  <Target Name="RestoreSignalR" BeforeTargets="RestorePackages">
    <PropertyGroup>
      <SignalRRepoPath>$(SolutionDir)..\SignalR\</SignalRRepoPath>
      <_SignalRPackagePath>$(SignalRRepoPath)\artifacts\$(Configuration)</_SignalRPackagePath>
      <SignalRPackagePath>$([System.IO.Path]::GetFullPath('$(_SignalRPackagePath)'))</SignalRPackagePath>
      <SignalRInstallCommand>"$(NuGetExePath)" install SignalR -source "$(SignalRPackagePath);https://go.microsoft.com/fwlink/?LinkID=230477" -o "$(PackagesDir)"</SignalRInstallCommand>
    </PropertyGroup>
    <Message Text="Restoring SignalR package from local source $(SignalRPackagePath)" />
    <ItemGroup>
      <SignalRPackageFiles Include="$(PackagesDir)\SignalR.*\**\*" />
    </ItemGroup>
    <Message Text="Deleting current SignalR package $(SignalRPackageFiles)" />
    <Delete Files="@(SignalRPackageFiles)" />
    <Message Text="Running $(SignalRInstallCommand)" />
    <Exec Command="$(SignalRInstallCommand)" Condition="Exists('$(SignalRPackagePath)')" />
  </Target>
